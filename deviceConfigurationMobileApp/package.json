{
  "name": "deviceConfigurationMobileApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "xcode": "xed -b ios",
    "build": "cd android && ./gradlew assembleRelease",
    "clean": "cd android && ./gradlew clean",
    "open": "open android/app/build/outputs/apk/release/",
    "openBundle": "open android/app/build/outputs/bundle/release/",
    "aab": "cd android && ./gradlew bundleRelease",
    "pods": "cd ios && pod install && cd ..",
    "coverage": "jest --coverage",
    "env:prod": "node ./src/config/scripts/set-environment.js production",
    "env:development": "node ./src/config/scripts/set-environment.js development",
    "env:qa": "node ./src/config/scripts/set-environment.js qa",
    "reset": "react-native start --reset-cache",
    "stop": "lsof -ti:8081 | xargs kill -9 || true",
    "clean-all": "yarn stop && watchman watch-del-all && rm -rf node_modules && yarn install && cd android && ./gradlew clean && cd ..",
    "fresh-start": "yarn stop && watchman watch-del-all && yarn start --reset-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-native/new-app-screen": "0.80.0",
    "@react-navigation/bottom-tabs": "^6.x",
    "@react-navigation/material-top-tabs": "^6.x",
    "@react-navigation/native": "^6.x",
    "@react-navigation/native-stack": "^6.x",
    "@sentry/cli": "^2.57.0",
    "@sentry/react-native": "^7.4.0",
    "deprecated-react-native-prop-types": "^5.0.0",
    "i18n-js": "^3.8.0",
    "lodash.memoize": "^4.1.2",
    "lottie-react-native": "^7.3.4",
    "md5": "^2.3.0",
    "react": "19.1.0",
    "react-native": "0.80.0",
    "react-native-base64": "^0.2.1",
    "react-native-ble-plx": "2.0.3",
    "react-native-camera": "^4.2.1",
    "react-native-dash": "^0.0.11",
    "react-native-device-info": "^14.1.1",
    "react-native-exit-app": "^1.1.0",
    "react-native-fs": "^2.20.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-localize": "^2.2.3",
    "react-native-mail": "^6.1.1",
    "react-native-modalbox": "^2.0.2",
    "react-native-pager-view": "^6.3.3",
    "react-native-permissions": "3.6.1",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-quick-base64": "^2.2.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.18.0",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.14.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-tab-view": "^3.3.0",
    "react-native-toast-message": "^2.3.3",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.0",
    "@react-native/eslint-config": "0.80.0",
    "@react-native/metro-config": "0.80.0",
    "@react-native/typescript-config": "0.80.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-navigation|react-native-quick-base64|react-native-base64|react-native-ble-plx|react-native-device-info|react-native-localize|react-native-permissions|react-native-splash-screen|react-native-keyboard-aware-scroll-view|react-native-toast-message|react-native-qrcode-scanner|react-native-fs|react-native-dash|react-native-skeleton-placeholder|react-native-mail|react-native-exit-app|lottie-react-native|react-native-pager-view|react-native-tab-view)/)"
    ],
    "moduleNameMapper": {
      "\\.(png|jpg|ico|jpeg|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/__mocks__/image-mock.js"
    },
    "testMatch": [
      "**/?(*-)+(test).[jt]s?(x)"
    ],
    "verbose": false,
    "coverageDirectory": "<rootDir>/__tests__/coverage",
    "setupFiles": [
      "<rootDir>/__tests__/__mocks__/navigation-mock.js",
      "<rootDir>/__tests__/__mocks__/react-redux-mock.js",
      "<rootDir>/__tests__/__mocks__/asyncStorage-mock.js",
      "<rootDir>/__tests__/__mocks__/redux-persist-mock.js",
      "<rootDir>/__tests__/__mocks__/lottie-react-native-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-base64-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-ble-plx-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-exit-app-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-localize-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-device-info-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-permissions-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-splash-screen-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-keyboard-aware-scroll-view-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-toast-message-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-qrcode-scanner-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-fs-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-dash-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-skeleton-placeholder-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-mail-mock.js",
      "<rootDir>/__tests__/__mocks__/react-native-backhandler-mock.js",
      "<rootDir>/__tests__/__mocks__/sentry-mock.js",
      "<rootDir>/__tests__/__mocks__/utils-mock.js"
    ]
  }
}